{
	"name": "eslint-config-mahir",
	"version": "1.0.7",
	"description": "The ultimate ESLint shareable config",
	"type": "module",
	"scripts": {
		"lint": "eslint --fix --cache .",
		"format": "biome check --write .",
		"build": "tsup",
		"prepack": "pnpm build"
	},
	"exports": {
		"./biome": {
			"types": "./dist/biome.d.ts",
			"import": "./dist/biome.js"
		},
		"./common": {
			"types": "./dist/common.d.ts",
			"import": "./dist/common.js"
		},
		"./edge": {
			"types": "./dist/edge.d.ts",
			"import": "./dist/edge.js"
		},
		"./jsdoc": {
			"types": "./dist/jsdoc.d.ts",
			"import": "./dist/jsdoc.js"
		},
		"./jsx": {
			"types": "./dist/jsx.d.ts",
			"import": "./dist/jsx.js"
		},
		"./mdx": {
			"types": "./dist/mdx.d.ts",
			"import": "./dist/mdx.js"
		},
		"./module": {
			"types": "./dist/module.d.ts",
			"import": "./dist/module.js"
		},
		"./native": {
			"types": "./dist/native.d.ts",
			"import": "./dist/native.js"
		},
		"./next": {
			"types": "./dist/next.d.ts",
			"import": "./dist/next.js"
		},
		"./node": {
			"types": "./dist/node.d.ts",
			"import": "./dist/node.js"
		},
		"./react": {
			"types": "./dist/react.d.ts",
			"import": "./dist/react.js"
		},
		"./tsdoc": {
			"types": "./dist/tsdoc.d.ts",
			"import": "./dist/tsdoc.js"
		},
		"./typescript": {
			"types": "./dist/typescript.d.ts",
			"import": "./dist/typescript.js"
		}
	},
	"keywords": [
		"eslint",
		"config",
		"typescript",
		"eslintconfig",
		"lint",
		"linter",
		"validate"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/imranbarbhuiya/eslint-config-mahir.git"
	},
	"bugs": {
		"url": "https://github.com/imranbarbhuiya/eslint-config-mahir/issues"
	},
	"author": "Parbez <imranbarbhuiya.fsd@gmail.com>",
	"license": "MIT",
	"files": [
		"dist/**"
	],
	"dependencies": {
		"@eslint/compat": "^1.2.7",
		"@eslint/eslintrc": "^3.3.0",
		"@eslint/js": "^9.21.0",
		"@next/eslint-plugin-next": "^15.2.0",
		"@typescript-eslint/eslint-plugin": "^8.26.0",
		"@typescript-eslint/parser": "^8.26.0",
		"eslint": "^9.21.0",
		"eslint-import-resolver-typescript": "^3.8.3",
		"eslint-mdx": "^3.1.5",
		"eslint-plugin-import-x": "^4.6.1",
		"eslint-plugin-jsdoc": "^50.6.3",
		"eslint-plugin-mdx": "^3.1.5",
		"eslint-plugin-n": "^17.16.1",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-react-compiler": "19.0.0-beta-40c6c23-20250301",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-sonarjs": "^3.0.2",
		"eslint-plugin-tsdoc": "^0.4.0",
		"eslint-plugin-typescript-sort-keys": "^3.3.0",
		"eslint-plugin-unicorn": "^57.0.0",
		"globals": "^16.0.0",
		"typescript-eslint": "^8.26.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^22.13.8",
		"@typescript-eslint/utils": "^8.26.0",
		"prettier": "^3.5.3",
		"tsup": "^8.4.0",
		"typescript": "^5.8.2"
	},
	"packageManager": "pnpm@10.5.2+sha512.da9dc28cd3ff40d0592188235ab25d3202add8a207afbedc682220e4a0029ffbff4562102b9e6e46b4e3f9e8bd53e6d05de48544b0c57d4b0179e22c76d1199b",
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"esbuild"
		]
	}
}
