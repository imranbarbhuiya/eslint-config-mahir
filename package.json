{
	"name": "eslint-config-mahir",
	"version": "0.0.46",
	"description": "The ultimate ESLint shareable config",
	"scripts": {
		"lint": "eslint --fix --cache .",
		"format": "biome check --write ."
	},
	"exports": {
		"./biome": {
			"import": {
				"types": "./dist/esm/biome.d.mts",
				"default": "./dist/esm/biome.mjs"
			},
			"require": {
				"types": "./dist/cjs/biome.d.cts",
				"default": "./dist/cjs/biome.cjs"
			}
		},
		"./common": {
			"import": {
				"types": "./dist/esm/common.d.mts",
