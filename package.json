{
	"name": "eslint-config-mahir",
	"version": "1.0.8",
	"description": "The ultimate ESLint shareable config",
	"type": "module",
	"scripts": {
		"lint": "eslint --fix --cache .",
		"format": "prettier --write --cache --experimental-cli .",
		"build": "tsdown",
		"prepack": "bun run build"
	},
	"exports": {
		"./common": {
			"types": "./dist/common.d.ts",
			"import": "./dist/common.js"
		},
		"./edge": {
			"types": "./dist/edge.d.ts",
			"import": "./dist/edge.js"
		},
		"./jsdoc": {
			"types": "./dist/jsdoc.d.ts",
			"import": "./dist/jsdoc.js"
		},
		"./jsx": {
			"types": "./dist/jsx.d.ts",
			"import": "./dist/jsx.js"
		},
		"./mdx": {
			"types": "./dist/mdx.d.ts",
			"import": "./dist/mdx.js"
		},
		"./module": {
			"types": "./dist/module.d.ts",
			"import": "./dist/module.js"
		},
		"./native": {
			"types": "./dist/native.d.ts",
			"import": "./dist/native.js"
		},
		"./next": {
			"types": "./dist/next.d.ts",
			"import": "./dist/next.js"
		},
		"./node": {
			"types": "./dist/node.d.ts",
			"import": "./dist/node.js"
		},
		"./react": {
			"types": "./dist/react.d.ts",
			"import": "./dist/react.js"
		},
		"./tsdoc": {
			"types": "./dist/tsdoc.d.ts",
			"import": "./dist/tsdoc.js"
		},
		"./typescript": {
			"types": "./dist/typescript.d.ts",
			"import": "./dist/typescript.js"
		}
	},
	"keywords": [
		"eslint",
		"config",
		"typescript",
		"eslintconfig",
		"lint",
		"linter",
		"validate"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/imranbarbhuiya/eslint-config-mahir.git"
	},
	"bugs": {
		"url": "https://github.com/imranbarbhuiya/eslint-config-mahir/issues"
	},
	"author": "Parbez <imranbarbhuiya.fsd@gmail.com>",
	"license": "MIT",
	"files": [
		"dist/**"
	],
	"dependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.34.0",
		"@next/eslint-plugin-next": "^15.5.0",
		"@typescript-eslint/eslint-plugin": "^8.41.0",
		"@typescript-eslint/parser": "^8.41.0",
		"eslint": "^9.34.0",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-mdx": "^3.6.2",
		"eslint-plugin-import-x": "^4.16.1",
		"eslint-plugin-jsdoc": "^54.1.1",
		"eslint-plugin-mdx": "^3.6.2",
		"eslint-plugin-n": "^17.21.3",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-compiler": "19.1.0-rc.2",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-sonarjs": "^3.0.5",
		"eslint-plugin-tsdoc": "^0.4.0",
		"eslint-plugin-typescript-sort-keys": "^3.3.0",
		"eslint-plugin-unicorn": "^60.0.0",
		"globals": "^16.3.0",
		"typescript-eslint": "^8.41.0"
	},
	"devDependencies": {
		"@prettier/plugin-oxc": "^0.0.4",
		"@types/node": "^24.3.0",
		"@typescript-eslint/utils": "^8.41.0",
		"prettier": "^3.6.2",
		"tsdown": "^0.14.2",
		"typescript": "^5.9.2"
	},
	"trustedDependencies": [
		"unrs-resolver"
	]
}
