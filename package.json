{
	"name": "eslint-config-mahir",
	"version": "1.0.7",
	"description": "The ultimate ESLint shareable config",
	"type": "module",
	"scripts": {
		"lint": "eslint --fix --cache .",
		"format": "biome check --write .",
		"build": "tsup",
		"prepack": "pnpm build"
	},
	"exports": {
		"./biome": {
			"types": "./dist/biome.d.ts",
			"import": "./dist/biome.js"
		},
		"./common": {
			"types": "./dist/common.d.ts",
			"import": "./dist/common.js"
		},
		"./edge": {
			"types": "./dist/edge.d.ts",
			"import": "./dist/edge.js"
		},
		"./jsdoc": {
			"types": "./dist/jsdoc.d.ts",
			"import": "./dist/jsdoc.js"
		},
		"./jsx": {
			"types": "./dist/jsx.d.ts",
			"import": "./dist/jsx.js"
		},
		"./mdx": {
			"types": "./dist/mdx.d.ts",
			"import": "./dist/mdx.js"
		},
		"./module": {
			"types": "./dist/module.d.ts",
			"import": "./dist/module.js"
		},
		"./native": {
			"types": "./dist/native.d.ts",
			"import": "./dist/native.js"
		},
		"./next": {
			"types": "./dist/next.d.ts",
			"import": "./dist/next.js"
		},
		"./node": {
			"types": "./dist/node.d.ts",
			"import": "./dist/node.js"
		},
		"./react": {
			"types": "./dist/react.d.ts",
			"import": "./dist/react.js"
		},
		"./tsdoc": {
			"types": "./dist/tsdoc.d.ts",
			"import": "./dist/tsdoc.js"
		},
		"./typescript": {
			"types": "./dist/typescript.d.ts",
			"import": "./dist/typescript.js"
		}
	},
	"keywords": [
		"eslint",
		"config",
		"typescript",
		"eslintconfig",
		"lint",
		"linter",
		"validate"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/imranbarbhuiya/eslint-config-mahir.git"
	},
	"bugs": {
		"url": "https://github.com/imranbarbhuiya/eslint-config-mahir/issues"
	},
	"author": "Parbez <imranbarbhuiya.fsd@gmail.com>",
	"license": "MIT",
	"files": [
		"dist/**"
	],
	"dependencies": {
		"@eslint/compat": "^1.2.6",
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.20.0",
		"@next/eslint-plugin-next": "^15.1.7",
		"@typescript-eslint/eslint-plugin": "^8.24.0",
		"@typescript-eslint/parser": "^8.24.0",
		"eslint": "^9.20.1",
		"eslint-import-resolver-typescript": "^3.8.0",
		"eslint-mdx": "^3.1.5",
		"eslint-plugin-import-x": "^4.6.1",
		"eslint-plugin-jsdoc": "^50.6.3",
		"eslint-plugin-mdx": "^3.1.5",
		"eslint-plugin-n": "^17.15.1",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-react-compiler": "^19.0.0-beta-e552027-20250112",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-sonarjs": "^3.0.2",
		"eslint-plugin-tsdoc": "^0.4.0",
		"eslint-plugin-typescript-sort-keys": "^3.3.0",
		"eslint-plugin-unicorn": "^56.0.1",
		"globals": "^15.15.0",
		"typescript-eslint": "^8.24.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^22.13.4",
		"prettier": "^3.5.1",
		"tsup": "^8.3.6",
		"typescript": "^5.7.3"
	},
	"packageManager": "pnpm@9.15.5"
}
